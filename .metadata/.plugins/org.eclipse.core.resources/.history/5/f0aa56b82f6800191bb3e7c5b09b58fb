import fr.adaming.model.Employe;


public class ClassTest {
	
	public static void main(String[] args) {
		Employe e1 = new Employe("Le Vaguerese", "Etienne", 45000);
	
		//Declaration de la deuxieme variable
		Employe e2 = e1;
		
		//modifier le nom de e1
		e1.setNom("Labagn's");
		
		//afficher le nom de e2
		System.out.println(e2.getNom());
		
		//declaration d'une variable de type String
		String s = "TOTO a Paris, il se balade tout seul!!";
		
		//recuperation de la taille de s
		int tailles = s.length();
		System.out.println(tailles);
		
		//recuperation d'un caractere de s à un index donné
		System.out.println(s.charAt(8));
		
		//creer une enveloppe de type Integer
		Integer i = new Integer(25);
		
		int age = Integer.parseInt("25");
		
		int a = 45;
		
		//autoboxing
		i=a;
		
		//unboxing
		a=i;
		

	}

}
