package fr.adaming.test;

public class Exo2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String str = "Les chaussettes de l'archiduchesse sont elles seches ou archi seches";
		char a;
		int somme = 0 ;
		int l = str.length();
		for (int i = 1; i < l; i++) {
		a = str.charAt(i);
		if(a == 's'){
			somme = somme + 1;
		}
		System.out.println(somme);
	}
}
}
