package fr.adaming.model;

public class Calcul {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int x = 7;
		for (int j = 1; j<=7; j++){
			double somme = 1;
			double a = Math.pow(x,j);
			double somme = somme + a;
			
			System.out.println("La valeur est de : " + somme);
			
		}
		
	}
	}


