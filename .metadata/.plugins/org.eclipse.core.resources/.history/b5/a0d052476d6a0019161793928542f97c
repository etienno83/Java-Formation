package fr.adaming.test;

public class ClassTest {

	public static void main(String[] args) {
		
		//declaration d'un tableau en java
		String semaine[];
		
		//instancier le tableau
		semaine = new String[7];
		
		//stocker des valeurs dans le tableau
		semaine[0]="Lundi";
		semaine[0]="Mardi";
		semaine[0]="Mercredi";
		semaine[0]="Jeudi";
		semaine[0]="Vendredi";
		
		//creation d'un tableau vide puis instanciation
		int[] tab1 = new int[5];
		tab1[0] = 1;
		//et ainso de suite
		
		//creation d'un tableau initialisé
		int[] tab2 = {1,2,3,4,5};
		
		//remplir tableau grace à la boucle for
		for(int i = 0; i < tab1.length; i++){
			tab1[i] =101 + i;
			
		}
		
	}

}